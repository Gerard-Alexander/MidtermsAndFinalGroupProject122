public class Cirlce extends Shape {
    private double radius;

   public Circle(String name, double radius) {
        this.name = name;
        this.radius = radius;
    }
   public double area() {
        return Math.PI * Math.pow(radius, 2);
    }
    public double perimeter() {
        return 2 * Math.PI * radius;
    }
}
